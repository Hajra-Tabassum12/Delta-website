  <style>
    nav {
      padding: auto;
    }

    #cssmenu,
    #cssmenu ul,
    #cssmenu ul li,
    #cssmenu ul li a,
    #cssmenu #head-mobile {
      border: 0;
      list-style: none;
      line-height: 1;
      display: block;
      position: relative;
      -webkit-box-sizing: border-box;
      -moz-box-sizing: border-box;
      box-sizing: border-box;
      color: #4f5399;
      font-weight: 600;
    }

    #cssmenu:after,
    #cssmenu>ul:after {
      content: ".";
      display: block;
      clear: both;
      visibility: hidden;
      line-height: 0;
      height: 0
    }

    #cssmenu #head-mobile {
      display: none
    }

    #cssmenu>ul>li {
      display: inline-block;
      padding: 35px 12px;
    }

    #cssmenu>ul>li.has-sub>a:after {
      position: absolute;
      top: 22px;
      right: 11px;
      width: 8px;
      height: 2px;
      display: block;
      background: #ddd;
      content: ''
    }

    #cssmenu>ul>li.has-sub>a:before {
      position: absolute;
      top: 19px;
      right: 14px;
      display: block;
      width: 2px;
      height: 8px;
      background: #ddd;
      content: '';
      -webkit-transition: all .25s ease;
      -ms-transition: all .25s ease;
      transition: all .25s ease
    }

    #cssmenu>ul>li.has-sub:hover>a:before {
      top: 23px;
      height: 0
    }

    #cssmenu ul ul {
      position: absolute;
      left: -9999px;
      top: 100%;
      width: max-content;
      min-width: 180px;
      background-color: #424794;
      padding: 10px
    }

    #cssmenu ul ul li {
      height: 0;
      -webkit-transition: all .25s ease;
      -ms-transition: all .25s ease;
      transition: all .25s ease
    }

    #cssmenu ul ul li a:hover {
      color: #b5b9f9 !important;
    }

    #cssmenu li:hover>ul {
      left: auto
    }

    #cssmenu li:hover>ul>li {
      height: 40px
    }

    #cssmenu ul ul ul {
      margin-left: 100%;
      top: -10px;
    }

    #cssmenu ul ul li a {
      border-bottom: 0px solid rgba(150, 150, 150, 0.15);
      padding: 11px 15px;
      color: #fff;
      font-weight: 400;
    }

    #cssmenu ul ul li:last-child>a,
    #cssmenu ul ul li.last-item>a {
      border-bottom: 0
    }

    #cssmenu ul ul li:hover>a,
    #cssmenu ul ul li a:hover {
      color: #fff
    }

    #cssmenu ul ul li.has-sub>a:after {
      position: absolute;
      top: 16px;
      right: 11px;
      width: 8px;
      height: 2px;
      display: block;
      content: ''
    }

    #cssmenu ul ul li.has-sub>a:before {
      position: absolute;
      top: 13px;
      right: 14px;
      display: block;
      width: 2px;
      height: 8px;
      content: '';
      -webkit-transition: all .25s ease;
      -ms-transition: all .25s ease;
      transition: all .25s ease
    }

    #cssmenu ul ul>li.has-sub:hover>a:before {
      top: 17px;
      height: 0
    }

    #cssmenu ul ul li.has-sub:hover,
    #cssmenu ul li.has-sub ul li.has-sub ul li:hover {
      background: none;
    }

    #cssmenu ul ul ul li.active a {
      border-left: 1px solid #333
    }

    #cssmenu>ul>li.has-sub>ul>li.active>a,
    #cssmenu>ul ul>li.has-sub>ul>li.active>a {
      border-top: 0px solid #333
    }

    #cssmenu>ul>li.has-sub>a.dmenu:after,
    #cssmenu>ul>li.has-sub>a.dmenu:before {
      display: none;
    }

    .dmenu {
      padding-right: 6px;
    }

    #cssmenu ul ul li.has-sub>a:before,
    #cssmenu ul ul li.has-sub>a:after {
      background-color: #fff;
    }



